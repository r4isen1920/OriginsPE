                  {
                    "label_description_1_centerer@common.empty_panel": {
                      "size": [ "100%", "100%c" ],
                      "controls": [
                        {
                          "label_description_1@origin_common.text_label": {
                            "anchor_from": "left_middle",
                            "anchor_to": "left_middle",
                            "size": [ "default", "default" ],
                            "offset": [ 8, 0 ],
