  "tab_button_selection_control@common.empty_panel": {
    "size": [ 28, 31 ],
    "controls": [
      {
        "unselected@origin_options_menu.tab_button_control": {
          "$is_tab_selected": false,
          "layer": 14
        }
      },
      {
        "selected@origin_options_menu.tab_button_control": {
          "$is_tab_selected": true,
          "layer": 15,
          "bindings": [
            {
              "binding_type": "collection",
              "binding_name": "#student_button_text",
              "binding_collection_name": "student_buttons_collection"
            },
            {
              "binding_type": "view",
              "source_property_name": "$visible_binding",
              "target_property_name": "#visible"
            }
          ]
        }
      }
    ]
  },

  "tab_button_control@common.empty_panel": {
    "size": [ 28, 31 ],

    "$tab_button_texture_pos|default": "middle",
    "$tab_button_texture|default": "Left",
    "$is_tab_selected|default": false,

    "variables": [
      {
        "requires": "(($tab_button_texture_pos = 'top') and not $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'BackTopMost')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'BackTopMostHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontTopMost')"
      },
      {
        "requires": "(($tab_button_texture_pos = 'middle') and not $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'Back')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'BackHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'Front')"
      },
      {
        "requires": "(($tab_button_texture_pos = 'bottom') and not $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'BackBottomMost')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'BackBottomMostHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontBottomMost')"
      },

      {
        "requires": "(($tab_button_texture_pos = 'top') and $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontTopMost')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontTopMostHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontTopMost')"
      },
      {
        "requires": "(($tab_button_texture_pos = 'middle') and $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'Front')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'Front')"
      },
      {
        "requires": "(($tab_button_texture_pos = 'bottom') and $is_tab_selected)",
        "$default_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontBottomMost')",
        "$hover_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontBottomMostHover')",
        "$pressed_button_texture": "('textures/ui/Tab' + $tab_button_texture + 'FrontBottomMost')"
      }
    ],
    "controls": [
      {
        "button@common_buttons.light_text_button": {
          "$border_visible": false,

          "$button_type_panel": "common.empty_panel",
          "$pressed_button_name": "button.student_button",

          "size": [ "100%", "100%" ],
          "layer": 10,
          "bindings": [
            {
              "binding_type": "collection_details",
              "binding_collection_name": "student_buttons_collection",
              "binding_collection_prefix": "student_buttons"
            }
          ],
          "button_mappings": [
            {
              "ignored": "$is_tab_selected",
              "to_button_id": "$pressed_button_name",
              "mapping_type": "pressed"
            },
            {
              "ignored": "$is_tab_selected",
              "from_button_id": "button.menu_select",
              "to_button_id": "$pressed_button_name",
              "mapping_type": "pressed"
            },
            {
              "ignored": "$is_tab_selected",
              "from_button_id": "button.menu_ok",
              "to_button_id": "$pressed_button_name",
              "mapping_type": "focused"
            }
          ]
        }
      }
    ]
  },

